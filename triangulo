import java.util.Scanner;
 public class Main {

  public static void main(String[] args) {
    int a,b,c;
    System.out.print("Insira um numero : ");
    Scanner in = new Scanner(System.in);
c = in.nextInt();

for(a=1;a<=c;a++){
   for(b=1;b<=a;b++)
 System.out.print(b);
 System.out.println("");
  }  
  }
}
